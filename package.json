{
  "name": "mishka",
  "version": "13.0.0",
  "private": true,
  "description": "Личный проект «Мишка» от HTML Academy",
  "devDependencies": {
    "autoprefixer": "9.6.1",
    "browser-sync": "^2.27.10",
    "gulp": "4.0.2",
    "gulp-csso": "3.0.1",
    "gulp-htmlmin": "5.0.1",
    "gulp-imagemin": "6.0.0",
    "gulp-less": "4.0.1",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "8.0.0",
    "gulp-posthtml": "3.0.4",
    "gulp-rename": "1.4.0",
    "gulp-svgstore": "7.0.1",
    "gulp-uglify": "3.0.2",
    "gulp-webp": "4.0.1",
    "posthtml-include": "1.2.0",
    "run-sequence": "2.2.1",
    "stylelint": "10.1.0",
    "stylelint-config-htmlacademy": "0.1.4",
    "vinyl-paths": "3.0.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint 'source/less/**/*.less' --syntax less",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "npm run build && gulp serve"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/less/**/*.less"
  ],
  "engines": {
    "node": "8.9"
  }
}
